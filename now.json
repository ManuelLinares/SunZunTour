{
  "version": 2,
  "name": "sun-zun-tour",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(assets/.+|.+\\.css|.+\\.js)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*).html",
      "headers": {
        "cache-control": "public,max-age=0,must-revalidate"
      },
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public,max-age=0,must-revalidate"
      },
      "dest": "/index.html"
    }
  ],
  "alias": "testingsunzun.now.sh",
  "public": true,
  "build": {
    "env": {
      "PROD": "true",
      "PROD_URL": "sunzuntour.now.sh",
      "SGApiKey": "@test-sendgrid-api-key"
    }
  },
  "env": {
    "PROD": "true",
    "PROD_URL": "sunzuntour.now.sh",
    "SGApiKey": "@test-sendgrid-api-key"
  }
}